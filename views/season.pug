html
  head
    link(rel="stylesheet" href="/css/styles.css")
  body 
      h1 Kim's Convenience
      div  
          h2= "Season " + season.number 
          table
            tr
              th No.
              th Title
              th Director
            for episode in season.episodes
              tr
                td(id= "center")= episode.number
                td(class= "common")= episode.title
                td(class= "common")= episode.director
              tr
                td(id= "left" colspan="3")= episode.synopsis
                  